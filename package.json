{
  "name": "dinky.js",
  "version": "0.1.0",
  "description": "A JavaScript client for Derpibooru API",
  "main": "lib/Dinky.js",
  "repository": "https://github.com/octet-stream/dinky",
  "author": "Nick K. <nick.kruchinin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "node_modules/.bin/ava",
    "report": "node_modules/.bin/nyc yarn test",
    "report:html": "node_modules/.bin/nyc yarn test && node_modules/.bin/nyc report --reporter=html yarn test",
    "report:lcov": "node_modules/.bin/nyc yarn test && node_modules/.bin/nyc report --reporter=lcov yarn test",
    "report:ci": "yarn report:lcov && node_modules/.bin/codecov",
    "eslint": "node_modules/.bin/eslint lib/**/*.js test/**/*.js",
    "staged": "node_modules/.bin/lint-staged"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/**/__helper__/**"
    ]
  },
  "engines": {
    "node": ">=8.9.4 <9 || >=10.0.0"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "3.0.0",
    "ava": "1.2.0",
    "codecov": "3.1.0",
    "eslint": "5.12.1",
    "eslint-plugin-ava": "5.1.1",
    "husky": "1.3.1",
    "isomorphic-fetch": "2.2.1",
    "lint-staged": "8.1.1",
    "nyc": "13.1.0",
    "proxyquire": "^2.1.0",
    "sinon": "7.2.3"
  },
  "dependencies": {
    "camelcase-keys": "5.0.0",
    "node-fetch": "2.3.0"
  }
}
